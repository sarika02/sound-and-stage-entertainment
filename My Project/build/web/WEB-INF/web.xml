<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginCheck2</servlet-name>
        <servlet-class>controller.LoginCheck2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bookingcheckdetail</servlet-name>
        <servlet-class>controller.Bookingcheckdetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bookingregisterdetail</servlet-name>
        <servlet-class>controller.Bookingregisterdetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginCheck2</servlet-name>
        <url-pattern>/LoginCheck2</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>InsertRegisterDetail</servlet-name>
        <servlet-class>controller.InsertRegisterDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertRegisterDetail</servlet-name>
        <url-pattern>/InsertRegisterDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bookingcheckdetail</servlet-name>
        <url-pattern>/Bookingcheckdetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bookingregisterdetail</servlet-name>
        <url-pattern>/Bookingregisterdetail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
